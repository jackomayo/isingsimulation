#include "IsingParticle.h"
#include<stdlib.h>
#include<stdio.h>
#include<iostream>


in (void)
{
 IsingParticle MyParticle;
 cout<< "the initial spin is: " << MyParticle.spinValue())<< endl;
 MyParticle.flipspin();
  cout<< "the flipped spin is: " << MyParticle.spinValue())<< endl; 
    
    
 system ("pause");   
 return 0;    
}